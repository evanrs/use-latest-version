{
  "name": "use-latest-version",
  "version": "1.1.3",
  "description": "React setState hook that guarantees latest verison with out of order updates.",
  "main": "dist/use-latest-version.js",
  "umd:main": "dist/use-latest-version.umd.js",
  "module": "dist/use-latest-version.module.js",
  "jsnext:main": "dist/use-latest-version.module.js",
  "react-native": "dist/use-latest-version.module.js",
  "types": "./dist",
  "typings": "dist/index.d.ts",
  "source": "src/index.ts",
  "scripts": {
    "clean": "npx rimraf dist",
    "test": "jest",
    "test:watch": "jest --watch",
    "prepare": "npm run build",
    "build": "microbundle",
    "release": "npx standard-version"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/evanrs/use-latest-version.git"
  },
  "files": [
    "dist/"
  ],
  "keywords": [
    "react-hooks",
    "hooks",
    "react",
    "setState",
    "versioning"
  ],
  "author": "Evan Schneider <me@evanrs.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/evanrs/use-latest-version/issues"
  },
  "homepage": "https://github.com/evanrs/use-latest-version#readme",
  "peerDependencies": {
    "react": ">=16.6.0"
  },
  "dependencies": {
    "ulid": "2.3.0"
  },
  "devDependencies": {
    "@testing-library/react-hooks": "3.2.1",
    "@types/jest": "25.1.2",
    "@types/react": "16.9.19",
    "@types/react-test-renderer": "16.9.2",
    "@types/testing-library__react-hooks": "3.2.0",
    "jest": "25.1.0",
    "microbundle": "0.12.0-next.8",
    "react": "16.12.0",
    "react-test-renderer": "16.12.0",
    "ts-jest": "25.2.0",
    "typescript": "3.7.5"
  },
  "jest": {
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.{ts,tsx}",
      "!**/node_modules/**"
    ],
    "testMatch": [
      "**/*.spec.ts",
      "**/*.test.ts"
    ]
  }
